    var ansP = document.getElementById("staff_appraisal_e2g1_percent");
    ansP.value = (((e2g1q1*1) + (e2g1q2*1) + (e2g1q3*1) + (e2g1q4*1) + (e2g1q5*1))*0.02)*50;

