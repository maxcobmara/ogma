    var ansK = document.getElementById("staff_appraisal_e2g2_percent");
    ansK.value = (((e2g2q1*1) + (e2g2q2*1) + (e2g2q3*1))*0.0333333333)*25;
  });

  $(".e2g3").keyup(function() {  
